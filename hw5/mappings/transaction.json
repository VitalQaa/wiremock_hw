{
    "request": {
        "urlPathPattern": "/transaction",
        "queryParameters": {
            "price": {
                "matches": "[0-9]*"
            },
            "date": {
                "matches": "[0-9]{4}-(0[1-9]|1[0-2])-(0[1-9]|[12][0-9]|3[01])$"
            }
        },
        "method": "GET"
    },
    "response": {
        "status": 200,
        "body": "success"
    }
}