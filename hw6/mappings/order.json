{
	"request": { 
		"urlPathPattern": "/order",
		"method": "GET",
	"bodyPatterns": [ 
		{
			"matchesJsonPath": "$[?(@.order=~/[0-9]{3}-[0-9]{2}-([0-9]{2}||[a-zA-Z]{2})/)]" 
		}
	]
	},
    "response": {
        "body": "{{{request.body}}}",
        "transformers": ["response-template"],
        "status" : 200,
		"delayDistribution": {
			"type": "uniform",
			"lower": 290,
			"upper": 310
		}
    }
}